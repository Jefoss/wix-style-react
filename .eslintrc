{
    "parser": "babel-eslint",
    "parserOptions":{
        "ecmaFeatures": {
            "modules": true,
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "expect": true,
        "protractor": true,
        "browser": true,
        "jest": true,
        "mapboxgl": true,
        "Q": true,
        "google": true,
        "FB": true,
        "goog": true,
        "$": true,
        "moment": true,
        "dispatch": true,
        "connect": true,
        "React": true,
        "ReactDOM": true,
        "spiceProduction": true,
        "ReactBootstrap": true,
        "Wix": true,
        "If": true,
        "For": true,
        "Else": true,
        "define": true,
        "olark": true,
        "Reflux": true,
        "uniqueId": true,
        "describe": false,
        "it":false,
        "before":false,
        "beforeEach":false,
        "afterEach":false,
        "spiceVer": false,
        "prodLocaleFile": false,
        "prodAppFile": "false",
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "strict": 0,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 0,
        "no-unused-vars": 1,
        "no-undef": 1,
        "react/no-find-dom-node": 0
    }
}
